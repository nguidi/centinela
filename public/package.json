{
  "name": "centinela",
  "version": "0.0.0",
  "description": "Sistema de Planificacion de Rutas de Vuelo de UAVs",
  "homepage": "https://github.com/nguidi/centinela",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IDS/centinela.git"
  },
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "private": true,
  "scripts": {
    "test": "testee test.html --browsers firefox --reporter Spec",
    "start": "done-serve --port 3030",
    "develop": "done-serve --develop --port 3030",
    "build": "node build"
  },
  "main": "centinela/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "donejs-app"
  ],
  "steal": {
    "main": "centinela/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "node_modules/can-zone/register"
    ],
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": ""
  },
  "dependencies": {
    "can-component": "^3.0.7",
    "can-connect": "^1.3.8",
    "can-connect-feathers": "^3.5.5",
    "can-define": "^1.0.17",
    "can-route": "^3.0.8",
    "can-route-pushstate": "^3.0.3",
    "can-set": "^1.1.0",
    "can-stache": "^3.0.20",
    "can-view-autorender": "^3.0.4",
    "can-zone": "^0.6.6",
    "done-autorender": "^1.0.0",
    "done-component": "^1.0.0",
    "done-css": "^3.0.0",
    "done-serve": "^1.0.0",
    "feathers": "^2.1.2",
    "feathers-authentication-client": "^0.3.2",
    "feathers-hooks": "^2.0.1",
    "feathers-socketio": "^1.6.0",
    "generator-donejs": "^1.0.0",
    "socket.io-client": "^2.0.1",
    "steal": "^1.3.1",
    "steal-less": "^1.2.0",
    "steal-socket.io": "^4.0.9",
    "steal-stache": "^3.0.5"
  },
  "devDependencies": {
    "donejs-cli": "^1.0.0",
    "funcunit": "^3.2.0",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.2.0",
    "testee": "^0.3.0",
    "can-fixture": "^1.0.13"
  },
  "license": "MIT"
}
